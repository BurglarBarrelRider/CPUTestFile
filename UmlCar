@startuml

class Computer {
    - Computername: String
    - OC: String
    - year: int
    - price: double
    + Computer()
    + Computer(Computername: String, OC: String, year: int, price: double)
    + displayInfo(): void
    + start(): void
    + upgradeComputer(feature: String, additionalCost: double): void
}

@enduml
